cess: (result) => {\n            const { user, tokens } = result\n            set((state) => ({\n              ...state,\n              user: AuthUserSchema.parse(user),\n              tokens: AuthTokensSchema.parse(tokens),\n              isAuthenticated: true,\n              error: null,\n              lastUpdated: Date.now(),\n            }))\n            setCookie(ACCESS_TOKEN, JSON.stringify(tokens.accessToken))\n          },\n          onError: (error) => {\n            set((state) => ({\n              ...state,\n              error: error.message,\n              isAuthenticated: false,\n            }))\n          },\n        }\n      )\n\n      return {\n        user: null,\n        tokens: { accessToken: initToken },\n        isLoading: false,\n        error: null,\n        isAuthenticated: false,\n        lastUpdated: null,\n\n        setUser: (user) => {\n          const validatedUser = user ? AuthUserSchema.parse(user) : null\n          set((state) => ({\n            ...state,\n            user: validatedUser,\n            isAuthenticated: !!validatedUser,\n            lastUpdated: Date.now(),\n          }))\n        },\n\n        setTokens: (tokens) => {\n          const validatedTokens = AuthTokensSchema.parse(tokens)\n          set((state) => ({\n            ...state,\n            tokens: validatedTokens,\n            isAuthenticated: true,\n            lastUpdated: Date.now(),\n          }))\n        },\n\n        setAccessToken: (accessToken) => {\n          set((state) => ({\n            ...state,\n            tokens: state.tokens ? { ...state.tokens, accessToken } : { accessToken },\n            isAuthenticated: !!accessToken,\n            lastUpdated: Date.now(),\n          }))\n          setCookie(ACCESS_TOKEN, JSON.stringify(accessToken))\n        },\n\n        resetAccessToken: () => {\n          set((state) => ({\n            ...state,\n            tokens: state.tokens ? { ...state.tokens, accessToken: '' } : null,\n            isAuthenticated: false,\n            lastUpdated: Date.now(),\n          }))\n          removeCookie(ACCESS_TOKEN)\n        },\n\n        setLoading: (isLoading) => set({ isLoading, lastUpdated: Date.now() }),\n        setError: (error) => set({ error, lastUpdated: Date.now() }),\n\n        login: async (credentials) => {\n          const { setLoading } = get()\n          await asyncLogin(setLoading, () => {}, (error) => get().setError(error))(credentials)\n        },\n\n        logout: () => {\n          removeCookie(ACCESS_TOKEN)\n          storage.removeValue()\n          set({\n            user: null,\n            tokens: null,\n            isLoading: false,\n            error: null,\n            isAuthenticated: false,\n            lastUpdated: Date.now(),\n          })\n        },\n\n        checkAuth: () => {\n          const { user, tokens } = get()\n          const isValidUser = user && AuthUserSchema.safeParse(user).success\n          const isValidToken = tokens && AuthTokensSchema.safeParse(tokens).success\n          return !!(isValidUser && isValidToken && tokens?.accessToken)\n        },\n\n        refreshAuth: async () => {\n          const { checkAuth, tokens } = get()\n          if (!checkAuth() || !tokens?.refreshToken) {\n            get().logout()\n            return\n          }\n\n          try {\n            await new Promise(resolve => setTimeout(resolve, 1000))\n            const newTokens = {\n              ...tokens,\n              accessToken: 'refreshed-access-token',\n              expiresAt: Date.now() + 24 * 60 * 60 * 1000,\n            }\n            get().setTokens(newTokens)\n          } catch (error) {\n            get().setError('Failed to refresh authentication')\n            get().logout()\n          }\n        },\n\n        reset: () => {\n          removeCookie(ACCESS_TOKEN)\n          storage.removeValue()\n          set({\n            user: null,\n            tokens: null,\n            isLoading: false,\n            error: null,\n         