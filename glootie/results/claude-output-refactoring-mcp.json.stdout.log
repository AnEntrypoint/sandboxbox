{"type":"assistant","message":{"id":"msg_20250926195648e69f361430684733","type":"message","role":"assistant","model":"glm-4.5","content":[{"type":"tool_use","id":"call_mv9idn0x1yj","name":"Write","input":{"file_path":"/config/workspace/optimized-test-1758887759363-mcp-refactoring/src/stores/base-store.ts","content":"import { create } from 'zustand'\nimport { persist } from 'zustand/middleware'\nimport { z } from 'zod'\n\nexport interface BaseStoreState<T> {\n  data: T | null\n  isLoading: boolean\n  error: string | null\n  lastUpdated: number | null\n}\n\nexport interface BaseStoreActions<T> {\n  setData: (data: T) => void\n  setLoading: (loading: boolean) => void\n  setError: (error: string | null) => void\n  reset: () => void\n  updateLastUpdated: () => void\n}\n\nexport function createBaseStore<T>(\n  initialState: BaseStoreState<T>,\n  schema: z.ZodType<T>\n) {\n  return create<BaseStoreState<T> & BaseStoreActions<T>>((set, get) => ({\n    ...initialState,\n    setData: (data) => {\n      const validatedData = schema.parse(data)\n      set({ data: validatedData, error: null, lastUpdated: Date.now() })\n    },\n    setLoading: (isLoading) => set({ isLoading }),\n    setError: (error) => set({ error }),\n    reset: () => set(initialState),\n    updateLastUpdated: () => set({ lastUpdated: Date.now() }),\n  }))\n}\n\nexport function createPersistentStore<T>(\n  initialState: BaseStoreState<T>,\n  schema: z.ZodType<T>,\n  storageKey: string\n) {\n  return create<BaseStoreState<T> & BaseStoreActions<T>>()(\n    persist(\n      (set, get) => ({\n        ...initialState,\n        setData: (data) => {\n          const validatedData = schema.parse(data)\n          set({ data: validatedData, error: null, lastUpdated: Date.now() })\n        },\n        setLoading: (isLoading) => set({ isLoading }),\n        setError: (error) => set({ error }),\n        reset: () => set(initialState),\n        updateLastUpdated: () => set({ lastUpdated: Date.now() }),\n      }),\n      {\n        name: storageKey,\n        partialize: (state) => ({\n          data: state.data,\n          lastUpdated: state.lastUpdated,\n        }),\n      }\n    )\n  )\n}"}}],"stop_reason":"tool_use","stop_sequence":null,"usage":{"input_tokens":422,"output_tokens":822,"cache_read_input_tokens":35921}},"parent_tool_use_id":null,"session_id":"ea3b70fd-130c-440d-8602-90050a39ea94","uuid":"3aecdd35-105d-44a4-bc2a-0ed7922187f5"}
