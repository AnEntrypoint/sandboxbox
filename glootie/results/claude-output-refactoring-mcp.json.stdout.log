{"type":"user","message":{"role":"user","content":[{"tool_use_id":"call_eog1drda29h","type":"tool_result","content":"     1â†’import { useState } from 'react'\n     2â†’import { z } from 'zod'\n     3â†’import { useForm } from 'react-hook-form'\n     4â†’import { zodResolver } from '@hookform/resolvers/zod'\n     5â†’import { Link, useNavigate } from '@tanstack/react-router'\n     6â†’import { Loader2, LogIn } from 'lucide-react'\n     7â†’import { toast } from 'sonner'\n     8â†’import { IconFacebook, IconGithub } from '@/assets/brand-icons'\n     9â†’import { useAuthStore } from '@/stores/auth-store'\n    10â†’import { sleep, cn } from '@/lib/utils'\n    11â†’import { Button } from '@/components/ui/button'\n    12â†’import {\n    13â†’  Form,\n    14â†’  FormControl,\n    15â†’  FormField,\n    16â†’  FormItem,\n    17â†’  FormLabel,\n    18â†’  FormMessage,\n    19â†’} from '@/components/ui/form'\n    20â†’import { Input } from '@/components/ui/input'\n    21â†’import { PasswordInput } from '@/components/password-input'\n    22â†’\n    23â†’const formSchema = z.object({\n    24â†’  email: z.email({\n    25â†’    error: (iss) => (iss.input === '' ? 'Please enter your email' : undefined),\n    26â†’  }),\n    27â†’  password: z\n    28â†’    .string()\n    29â†’    .min(1, 'Please enter your password')\n    30â†’    .min(7, 'Password must be at least 7 characters long'),\n    31â†’})\n    32â†’\n    33â†’interface UserAuthFormProps extends React.HTMLAttributes<HTMLFormElement> {\n    34â†’  redirectTo?: string\n    35â†’}\n    36â†’\n    37â†’export function UserAuthForm({\n    38â†’  className,\n    39â†’  redirectTo,\n    40â†’  ...props\n    41â†’}: UserAuthFormProps) {\n    42â†’  const [isLoading, setIsLoading] = useState(false)\n    43â†’  const navigate = useNavigate()\n    44â†’  const { auth } = useAuthStore()\n    45â†’\n    46â†’  const form = useForm<z.infer<typeof formSchema>>({\n    47â†’    resolver: zodResolver(formSchema),\n    48â†’    defaultValues: {\n    49â†’      email: '',\n    50â†’      password: '',\n    51â†’    },\n    52â†’  })\n    53â†’\n    54â†’  function onSubmit(data: z.infer<typeof formSchema>) {\n    55â†’    setIsLoading(true)\n    56â†’\n    57â†’    // Mock successful authentication\n    58â†’    const mockUser = {\n    59â†’      accountNo: 'ACC001',\n    60â†’      email: data.email,\n    61â†’      role: ['user'],\n    62â†’      exp: Date.now() + 24 * 60 * 60 * 1000, // 24 hours from now\n    63â†’    }\n    64â†’\n    65â†’    toast.promise(sleep(2000), {\n    66â†’      loading: 'Signing in...',\n    67â†’      success: () => {\n    68â†’        setIsLoading(false)\n    69â†’\n    70â†’        // Set user and access token\n    71â†’        auth.setUser(mockUser)\n    72â†’        auth.setAccessToken('mock-access-token')\n    73â†’\n    74â†’        // Redirect to the stored location or default to dashboard\n    75â†’        const targetPath = redirectTo || '/'\n    76â†’        navigate({ to: targetPath, replace: true })\n    77â†’\n    78â†’        return `Welcome back, ${data.email}!`\n    79â†’      },\n    80â†’      error: 'Error',\n    81â†’    })\n    82â†’  }\n    83â†’\n    84â†’  return (\n    85â†’    <Form {...form}>\n    86â†’      <form\n    87â†’        onSubmit={form.handleSubmit(onSubmit)}\n    88â†’        className={cn('grid gap-3', className)}\n    89â†’        {...props}\n    90â†’      >\n    91â†’        <FormField\n    92â†’          control={form.control}\n    93â†’          name='email'\n    94â†’          render={({ field }) => (\n    95â†’            <FormItem>\n    96â†’              <FormLabel>Email</FormLabel>\n    97â†’              <FormControl>\n    98â†’                <Input placeholder='name@example.com' {...field} />\n    99â†’              </FormControl>\n   100â†’              <FormMessage />\n   101â†’            </FormItem>\n   102â†’          )}\n   103â†’        />\n   104â†’        <FormField\n   105â†’          control={form.control}\n   106â†’          name='password'\n   107â†’          render={({ field }) => (\n   108â†’            <FormItem className='relative'>\n   109â†’              <FormLabel>Password</FormLabel>\n   110â†’              <FormControl>\n   111â†’                <PasswordInput placeholder='********' {...field} />\n   112â†’              </FormControl>\n   113â†’              <FormMessage />\n   114â†’              <Link\n   115â†’                to='/forgot-password'\n   116â†’                className='text-muted-foreground absolute end-0 -top-0.5 text-sm font-medium hover:opacity-75'\n   117â†’              >\n   118â†’                Forgot password?\n   119â†’              </Link>\n   120â†’            </FormItem>\n   121â†’          )}\n   122â†’        />\n   123â†’        <Button className='mt-2' disabled={isLoading}>\n   124â†’          {isLoading ? <Loader2 className='animate-spin' /> : <LogIn />}\n   125â†’          Sign in\n   126â†’        </Button>\n   127â†’\n   128â†’        <div className='relative my-2'>\n   129â†’          <div className='absolute inset-0 flex items-center'>\n   130â†’            <span className='w-full border-t' />\n   131â†’          </div>\n   132â†’          <div className='relative flex justify-center text-xs uppercase'>\n   133â†’            <span className='bg-background text-muted-foreground px-2'>\n   134â†’              Or continue with\n   135â†’            </span>\n   136â†’          </div>\n   137â†’        </div>\n   138â†’\n   139â†’        <div className='grid grid-cols-2 gap-2'>\n   140â†’          <Button variant='outline' type='button' disabled={isLoading}>\n   141â†’            <IconGithub className='h-4 w-4' /> GitHub\n   142â†’          </Button>\n   143â†’          <Button variant='outline' type='button' disabled={isLoading}>\n   144â†’            <IconFacebook className='h-4 w-4' /> Facebook\n   145â†’          </Button>\n   146â†’        </div>\n   147â†’      </form>\n   148â†’    </Form>\n   149â†’  )\n   150â†’}